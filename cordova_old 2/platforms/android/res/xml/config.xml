    <splash height="1536" src="platforms/ios/MEC_App/Resources/splash/splash.png" width="2048" />
    <splash height="1136" src="platforms/ios/MEC_App/Resources/splash/splash.png" width="640" />
    <splash height="1334" src="platforms/ios/MEC_App/Resources/splash/splash.png" width="750" />
    <splash height="2208" src="platforms/ios/MEC_App/Resources/splash/splash.png" width="1242" />
    <splash height="1242" src="platforms/ios/MEC_App/Resources/splash/splash.png" width="2208" />
</widget>
